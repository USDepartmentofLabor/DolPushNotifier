<div class="table-responsive">
  <table class="table table-striped table-bordered">
    <thead class="thead-dark">
      <tr>
        <th scope="col">Name</th>
        <th scope="col">APNS Development Certificate</th>
        <th scope="col">APNS Production Cetificate</th>
        <th scope="col">Sandbox Mode</th>
        <th scope="col">Created At</th>
        <th colspan="2"></th>
      </tr>
    </thead>
    <tbody>
      <tr>
        <td><%= @app.name %></td>
        <td><%= @app.apns_dev_cert %></td>
        <td><%= @app.apns_prod_cert %></td>
        <td><%= @app.sandbox_mode %></td>
        <td><%= @app.created_at %></td>
        <td><%= link_to 'Edit', edit_pushapp_path(@app), class: 'btn btn-dark' %></td>
        <td><%= link_to 'Delete', pushapp_path(@app), data: {confirm: 'Are you sure?'}, method: :delete, class: 'btn btn-danger'%></td>
      </tr>
    </tbody>
  </table>
</div>
